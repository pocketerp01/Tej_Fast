    <script type="text/javascript">
        $(document).ready(function () {
            calculateSum();
        });
        function pad(width, string, padding) {
            return (width <= string.length) ? string : pad(width, padding + string, padding)
        }
        function calculateSum() {
            cal_bom2();
        }
        function cal_bom1() {

        }
        function cal_bom2() {
            var gridCount = $("[id*=sg1].GridviewScrollItem2").length - 1;
            var dcnt = 0, lyrtot = 0, Lcode = "";
            var col1t = 0,col3t = 0, col4t = 0, col5t = 0, col6t = 0, col7t = 0, col8t = 0, col9t = 0, col10t = 0, col11t = 0, col12t = 0, col13t = 0;
            for (var i = 0; i < gridCount; i++) {
                var thickness = $("input[id*=sg1_t2]");
                var density = $("input[id*=sg1_t3]");
                var gsm_wet = $("input[id*=sg1_t4]");
                var solid_per = $("input[id*=sg1_t5]");
                var gsm_dry = $("input[id*=sg1_t6]");
                var percent = $("input[id*=sg1_t7]");
                var width = $("input[id*=sg1_t8]");
                var qty = $("input[id*=sg1_t9]");
                lyrtot = 0;
                Lcode = $("#ContentPlaceHolder1_sg1_ddLayer_" + i).val();

                col3t += ((thickness[i].value * 1));
                col5t += ((gsm_wet[i].value * 1));
                col6t += ((solid_per[i].value * 1));

                for (var k = 0; k < gridCount; k++) {
                    var thicknessk = $("input[id*=sg1_t2]");
                    var densityk = $("input[id*=sg1_t3]");
                    var gsm_dryk = $("input[id*=sg1_t4]");
                    var percentk = $("input[id*=sg1_t5]");
                    var qtyk = $("input[id*=sg1_t6]");

                    if (Lcode == $("#ContentPlaceHolder1_sg1_ddLayer_" + k).val()) {
                        lyrtot += (percentk[k].value * 1);
                    }
                    if ((lyrtot * 1) > 0)
                        document.getElementById('ContentPlaceHolder1_sg1_sg1_t6_' + k).value = (((percentk[k].value * 1) / 100) * 100).toFixed(2);
                }
                document.getElementById('ContentPlaceHolder1_sg1_sg1_t4_' + i).value = ((thickness[i].value * 1) * (density[i].value * 1)).toFixed(2);
            }
            for (var i = 0; i < gridCount; i++) {
                if (col6t > 0 && col5t > 0) {
                    debugger;
                    document.getElementById('ContentPlaceHolder1_sg1_sg1_t7_' + i).value = (((document.getElementById('ContentPlaceHolder1_sg1_sg1_t5_' + i).value * 1) / col6t) * (document.getElementById('ContentPlaceHolder1_txtQty').value * 1)).toFixed(2);
                    document.getElementById('ContentPlaceHolder1_sg1_sg1_t7_' + i).value = ((document.getElementById('ContentPlaceHolder1_sg1_sg1_t7_' + i).value * 1) * ((100 + (document.getElementById('ContentPlaceHolder1_TextBox3').value * 1)) / 100)).toFixed(2);
                    document.getElementById('ContentPlaceHolder1_sg1_sg1_t8_' + i).value = (((document.getElementById('ContentPlaceHolder1_sg1_sg1_t4_' + i).value * 1) / col5t) * 100).toFixed(2);
                    document.getElementById('ContentPlaceHolder1_sg1_sg1_t9_' + i).value = document.getElementById('ContentPlaceHolder1_sg1_sg1_t7_' + i).value;

                    document.getElementById('ContentPlaceHolder1_sg1_sg1_t11_' + i).value = document.getElementById('ContentPlaceHolder1_sg1_sg1_t9_' + i).value;
                }

                col1t += (document.getElementById('ContentPlaceHolder1_sg1_sg1_t1_' + i).value * 1);
                col4t += (document.getElementById('ContentPlaceHolder1_sg1_sg1_t4_' + i).value * 1);
                col8t += (document.getElementById('ContentPlaceHolder1_sg1_sg1_t7_' + i).value * 1);
                col9t += (document.getElementById('ContentPlaceHolder1_sg1_sg1_t8_' + i).value * 1);
                col11t += (document.getElementById('ContentPlaceHolder1_sg1_sg1_t10_' + i).value * 1);
                col12t += (document.getElementById('ContentPlaceHolder1_sg1_sg1_t11_' + i).value * 1);
                //col13t += (document.getElementById('ContentPlaceHolder1_sg1_sg1_t12_' + i).value * 1);
            }
            if (gridCount > 0) {
                document.getElementById('ContentPlaceHolder1_sg1_sg1_t1_' + (gridCount)).value = col1t.toFixed(2);
                document.getElementById('ContentPlaceHolder1_sg1_sg1_t2_' + (gridCount)).value = col3t.toFixed(2);
                document.getElementById('ContentPlaceHolder1_sg1_sg1_t4_' + (gridCount)).value = col4t.toFixed(2);
                document.getElementById('ContentPlaceHolder1_sg1_sg1_t5_' + (gridCount)).value = col5t.toFixed(2);
                document.getElementById('ContentPlaceHolder1_sg1_sg1_t9_' + (gridCount)).value = col6t.toFixed(2);

                document.getElementById('ContentPlaceHolder1_sg1_sg1_t7_' + (gridCount)).value = col8t.toFixed(2);
                document.getElementById('ContentPlaceHolder1_sg1_sg1_t8_' + (gridCount)).value = col9t.toFixed(2);
                document.getElementById('ContentPlaceHolder1_sg1_sg1_t10_' + (gridCount)).value = col11t.toFixed(2);
                document.getElementById('ContentPlaceHolder1_sg1_sg1_t11_' + (gridCount)).value = col12t.toFixed(2);
                //document.getElementById('ContentPlaceHolder1_sg1_sg1_t12_' + (gridCount)).value = col13t.toFixed(2);
            }
        }
